{
  "extends": "tslint:recommended",
    "rules": {
    "quotemark": false,
      "max-classes-per-file": [true, 2],
        "member-access": false,
          "only-arrow-functions": false,
            "align": [true, "parameters", "arguments", "members", "elements"],
              "no-var-keyword": false,
                "arrow-parens": false,
                  "prefer-const": false
  },
  "jsRules": {
    "quotemark": [true, "single", "double", "jsx-double"]
  }
}
